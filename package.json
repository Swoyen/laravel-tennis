        "vue-loader": "^15.9.5",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "bootstrap-icons": "^1.3.0",
        "laravel-vue-pagination": "^2.3.1",
        "vue": "^2.6.12",
        "vue-observe-visibility": "^1.0.0",
        "vue-router": "^3.5.1",
        "vuejs-datepicker": "^1.6.2"
    }
}
